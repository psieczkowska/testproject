  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Production",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--flavor",
        "production",
        "--dart-define=ENVIRONMENT=production",
        "--target",
        "lib/main.dart"
      ]
    },
    {
      "name": "Launch Staging",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--flavor",
        "staging",
        "--dart-define=ENVIRONMENT=staging",
        "--target",
        "lib/main.dart"
      ]
    },
    {
      "name": "Launch Dev",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--flavor",
        "dev",
        "--dart-define=ENVIRONMENT=dev",
        "--target",
        "lib/main.dart"
      ]
    }
  ]
}
